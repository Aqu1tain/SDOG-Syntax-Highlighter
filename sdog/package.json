{
  "name": "sdog",
  "displayName": "SDOG Syntax Highlighting",
  "description": "Provides Syntax Highlighting for SDOG Files",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "icon": "images/sea-dog-logo.png",
  "publisher": "Akitain",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sdog",
      "aliases": ["Seadog", "sdog"],
      "extensions": ["sdog","sdogx"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sdog",
      "scopeName": "source.sdog",
      "path": "./syntaxes/sdog.tmLanguage.json"
    }]
  }
}
